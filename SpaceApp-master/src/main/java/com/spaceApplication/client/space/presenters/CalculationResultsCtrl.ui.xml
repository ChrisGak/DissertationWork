<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:gwt='urn:import:com.google.gwt.user.client.ui'>
    <gwt:HTMLPanel>
        <div class="page-inner">
            <div class="page-title">
                <h3 class="breadcrumb-header">Результаты моделирования</h3>
            </div>
            <div id="main-wrapper">
                <div class="row">
                    <div class="col-lg-3 col-md-6">
                        <div class="panel panel-white stats-widget">
                            <gwt:HTMLPanel ui:field="forceValue" styleName="panel-body">
                            </gwt:HTMLPanel>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="panel panel-white stats-widget">
                            <gwt:HTMLPanel ui:field="momentValue" styleName="panel-body">
                            </gwt:HTMLPanel>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="panel panel-white stats-widget">
                            <gwt:HTMLPanel ui:field="transversalAccelerationValue" styleName="panel-body">
                            </gwt:HTMLPanel>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="panel panel-white stats-widget">
                            <gwt:HTMLPanel ui:field="radialAccelerationValue" styleName="panel-body">
                            </gwt:HTMLPanel>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-8 col-md-12">
                        <div class="panel panel-white">
                            <div class="panel-body">
                                <gwt:HTMLPanel ui:field="systemHeightChart" styleName="custom_chart">
                                </gwt:HTMLPanel>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-12">
                        <div class="panel panel-white">
                            <div class="panel-body">
                                <gwt:HTMLPanel ui:field="semimajorAxisChart" styleName="custom_chart">
                                </gwt:HTMLPanel>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-5 col-md-12">
                        <div class="panel panel-white">
                            <gwt:HTMLPanel ui:field="angleSpeedChart" styleName="custom_chart">
                            </gwt:HTMLPanel>
                        </div>
                    </div>
                    <div class="col-lg-7 col-md-12">
                        <div class="panel panel-white">
                            <gwt:HTMLPanel ui:field="trajectoryChart" styleName="panel-body">
                            </gwt:HTMLPanel>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-4 col-md-6">
                        <div class="panel panel-white">
                            <div class="panel-body">
                                <gwt:HTMLPanel ui:field="deflectionAngleChart" styleName="custom_chart">
                                </gwt:HTMLPanel>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="panel panel-white">
                            <div class="panel-body">
                                <gwt:HTMLPanel ui:field="trueAnomalyChart" styleName="custom_chart">
                                </gwt:HTMLPanel>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-12">
                        <div class="panel panel-white">
                            <div class="panel-body">
                                <gwt:HTMLPanel ui:field="eccentricityChart" styleName="custom_chart">
                                </gwt:HTMLPanel>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </gwt:HTMLPanel>
</ui:UiBinder>
